  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.11",
    "@types/mysql": "^2.15.21",
    "@types/node": "^20.4.8",
    "@types/node-cron": "^3.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^8.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "aws-sdk": "^2.1523.0",
    "bcrypt": "^5.1.0",
